{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Deny",
      "Action": [
        "s3:DeleteBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::*-tf-state-backend",
        "arn:aws:s3:::*-tf-state-backend/*.tfstate",
        "arn:aws:s3:::*-tf-state-backend/*.tfstate*"
      ]
    }
  ]
}
