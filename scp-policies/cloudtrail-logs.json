{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "PreventDeleteOfCloudtrailLogs",
      "Effect": "Deny",
      "Action": [
        "s3:DeleteBucket",
        "s3:DeleteObject",
        "s3:DeleteObjectVersion"
      ],
      "Resource": [
        "arn:aws:s3:::*-cloudtrail-event-history",
        "arn:aws:s3:::*-cloudtrail-event-history/*"
      ]
    }
  ]
}
